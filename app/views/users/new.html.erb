<form action="/users" method="POST">

    <label for="name">Name</label>
    <input id="name" type="text" name = user[name]></input>
    <br>
  
    <label for="email">Email</label>
    <input id="email" type="text" name = user[email]></input>
    <br>
  
    <label for="password">Password</label>
    <input id="password" type="password" name = user[password]></input>
    <br>
 

    <input id="submit" type="button" value="Create User"></input>
    <br>



</form>